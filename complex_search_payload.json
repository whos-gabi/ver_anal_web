{
    "filters": [
        {
            "attribute": "company_location",
            "relation": "in",
            "value": [
                {
                    "country": "United States"
                }
            ],
            "strictness": 2
        },
        {
            "attribute": "company_products",
            "relation": "match_expression",
            "value": {
                "match": {
                    "operator": "and",
                    "operands": [
                        {
                            "operator": "or",
                            "operands": [
                                "chemicals"
                            ]
                        },
                        {
                            "operator": "or",
                            "operands": [
                                "medical",
                                "pharmaceutical"
                            ]
                        }
                    ]
                }
            },
            "strictness": 1,
            "supplier_types": [
                "manufacturer",
                "distributor"
            ]
        }
    ]
}
